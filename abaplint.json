{
  "global": {
    "version": "v751",
    "skipGeneratedGatewayClasses": true
  },
  "rules": {
    "7bit_ascii": {
      "enabled": true
    },
    "contains_tab": {
      "enabled": true
    },
    "functional_writing": {
      "enabled": true,
      "ignoreExceptions": true
    },
    "line_length": {
      "enabled": true,
      "length": 170
    },
    "max_one_statement": {
      "enabled": true
    },
    "parser_error": {
      "enabled": true
    }
  }
}
